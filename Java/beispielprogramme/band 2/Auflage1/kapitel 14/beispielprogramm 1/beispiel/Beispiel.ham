public class Beispiel {
  private   int iPrivate;
            int iOhne;
  protected int iProtected;
  public    int iPublic;

  public Beispiel() {
    this.iPrivate = 0;
    this.iOhne = 1;
    this.iProtected = 2;
    this.iPublic = 3;
  }

  public Beispiel(Beispiel obj) {
